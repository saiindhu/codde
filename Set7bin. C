#include<stdio.h>
void main()
{

char a[10];
int i,l=0,temp =0;
scanf("%s",a);
for(i=0;a[i]!=NULL;i++ ) 
  {
     l++;
  }
for(i=0;i<l;i++)
{
if(a[i]=='1' || a[i]=='0') 
{
    temp=1;
    
}
else 
{
    temp=0;
break;
}
}
if (temp==1)
 {
    printf("yes");
 }
else
 {
    printf("No");

 }
getch ();
}
